{
  "name": "create-privy-app",
  "version": "1.0.0",
  "description": "CLI tool to create Next.js or Vite apps with Privy authentication, Wagmi integration, and global wallet support",
  "main": "dist/index.js",
  "bin": {
    "create-privy-app": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && node scripts/pre-publish-check.js",
    "publish-check": "node scripts/pre-publish-check.js",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "link": "npm link",
    "unlink": "npm unlink -g create-privy-app",
    "test:help": "tsx src/index.ts --help",
    "test:nextjs": "tsx src/index.ts my-nextjs-app --nextjs",
    "test:nextjs-wallets": "tsx src/index.ts my-nextjs-app --nextjs --global-wallets",
    "test:dry-run": "tsx src/index.ts test-app --dry-run --verbose",
    "test:cli": "npm run build && node dist/index.js --help"
  },
  "keywords": [
    "privy",
    "nextjs",
    "vite",
    "cli",
    "auth",
    "authentication",
    "wagmi",
    "wallet",
    "web3",
    "ethereum",
    "scaffold",
    "boilerplate",
    "template",
    "typescript",
    "react"
  ],
  "author": "Siddesh",
  "license": "MIT",
  "homepage": "https://github.com/Siddesh7/create-privy-app",
  "repository": {
    "type": "git",
    "url": "https://github.com/Siddesh7/create-privy-app.git"
  },
  "bugs": {
    "url": "https://github.com/Siddesh7/create-privy-app/issues"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^14.0.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.2.12",
    "inquirer-search-checkbox": "^1.0.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.19.11",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@9.15.4+sha512.b2dc20e2fc72b3e18848459b37359a32064663e5627a51e4c74b2c29dd8e8e0491483c3abb40789cfd578bf362fb6ba8261b05f0387d76792ed6e23ea3b1b6a0"
}
